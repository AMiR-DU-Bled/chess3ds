{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Citra GDB Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/morpion.elf",
            "miDebuggerServerAddress": "localhost:24689",
            "miDebuggerPath": "arm-none-eabi-gdb",
            "cwd": "${workspaceFolder}",
            "stopAtEntry": true,
            "setupCommands": [
                {
                    "description": "Enable pretty printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
